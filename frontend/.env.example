# Base API URL for EventSec backend (default derives from window.location + :8000)
VITE_API_BASE_URL=

# Optional WS override for threat map (if unset, derived from API base + /ws/threatmap)
VITE_THREATMAP_WS_URL=

# Base URL for Email Protection service (default derives from window.location + :8100)
VITE_EMAIL_PROTECT_BASE_URL=

# Use CTI mock adapter (true/false)
VITE_CTI_USE_MOCK=true

# Enable client debug logging (true/false)
VITE_UI_DEBUG=false

# Threat map stale detection threshold in ms (default 10000)
VITE_THREATMAP_STALE_MS=10000
